sql\erststimmekandidat=SELECT Vorname, Name, sum(1Anzahl) AS Gesamtstimmen FROM 1stimme, direktkandidaten WHERE 1stimme.D_ID = direktkandidaten.D_ID GROUP BY 1stimme.D_ID ORDER BY sum(1Anzahl) DESC;
sql\zweitstimmepartei=SELECT P_Bezeichnung, sum(2Anzahl) AS Gesamtstimmen FROM 2stimme, partei WHERE 2stimme.P_ID = partei.P_ID GROUP BY 2stimme.P_ID ORDER BY sum(2Anzahl) DESC;
